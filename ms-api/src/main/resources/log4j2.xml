<?xml version="1.0" encoding="UTF-8"?><Configuration>    <Properties>        <Property name="PATTERN">[%d{yyyy-MM-dd} %d{HH:mm:ss}] %highlight{[ %-5level ][%C{1}]} %m%n</Property>    </Properties>    <Appenders>        <Console name="Console" target="SYSTEM_OUT">            <PatternLayout disableAnsi="false"                           pattern="${PATTERN}"/>        </Console>        <RollingFile name="RollingFile" fileName="./logs/last.log"                     filePattern="./logs/%d{MM-yyyy}/logfile-%d{dd-MMMM}-%i.log">            <PatternLayout                    pattern="${PATTERN}"/>            <Policies>                <OnStartupTriggeringPolicy/>                <SizeBasedTriggeringPolicy size="5 MB"/>                <TimeBasedTriggeringPolicy interval="1"/>            </Policies>            <DefaultRolloverStrategy max="10"/>        </RollingFile>    </Appenders>    <Loggers>        <Root level="info">            <AppenderRef ref="Console"/>            <AppenderRef ref="RollingFile" level="warn"/>        </Root>    </Loggers></Configuration>